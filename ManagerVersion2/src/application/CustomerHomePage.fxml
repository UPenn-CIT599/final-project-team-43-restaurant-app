            <Font name="Arial Bold" size="19.0" />
         </font>
      </TextArea>
      <ImageView fitHeight="135.0" fitWidth="238.0" pickOnBounds="true" preserveRatio="true" translateX="400.0">
         <image>
            <Image url="@../../47f3108dbd755839083fa9e5c0bcacc0_happy-taco-clipart_920-535.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="149.0" fitWidth="293.0" pickOnBounds="true" preserveRatio="true" translateY="-120.0">
