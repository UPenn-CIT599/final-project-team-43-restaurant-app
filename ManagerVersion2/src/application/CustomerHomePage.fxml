<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button mnemonicParsing="false" text="About us" />
            <Button mnemonicParsing="false" text="View Menu" />
            <Button mnemonicParsing="false" text="Reservations" />
            <Button mnemonicParsing="false" text="Order Delivery" />
        </items>
      </ToolBar>
      <TextArea prefHeight="353.0" prefWidth="640.0" text="Welcome to our Taco Restaurant!">
