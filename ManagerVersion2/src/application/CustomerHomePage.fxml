            <Image url="@../../taco%20man.jpg" />
         </image>
      </ImageView>
      <ImageView fitHeight="178.0" fitWidth="240.0" pickOnBounds="true" preserveRatio="true" translateY="-100.0">
         <image>
            <Image url="@../../NationalTacoDay2016-1474926323-1.jpg" />
         </image>
      </ImageView>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Not a customer? Click below!" />
